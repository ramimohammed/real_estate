<p id="notice"><%= notice %></p>

<h1>Listing Xteants</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @xteants.each do |xteant| %>
      <tr>
        <td><%= xteant.first_name %></td>
        <td><%= xteant.last_name %></td>
        <td><%= xteant.phone %></td>
        <td><%= xteant.email %></td>
        <td><%= xteant.address %></td>
        <td><%= xteant.latitude %></td>
        <td><%= xteant.longitude %></td>
        <td><%= link_to 'Show', xteant %></td>
        <td><%= link_to 'Edit', edit_xteant_path(xteant) %></td>
        <td><%= link_to 'Destroy', xteant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Xteant', new_xteant_path %>
